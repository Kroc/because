.DEF	HI		$01
.DEF	LO		$00

.INC	"gb/gb.wla"			; GameBoy hardware definitions

.INC	"str.wla"			; string & text routines
.INC	"cards.wla"			; card data structures & routines

.INC	"gfx.wla"		NAMESPACE "gfx"

.SECTION        "game_init"

init:
