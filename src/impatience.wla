	ld 	SP,	$dff0		; locate the stack

	call	gfx.init
	
	call	acesup.init

	; TODO!
-	jr -
.ENDS

.INC	"playfield.wla"		NAMESPACE "playfield"
.INC	"game_acesup.wla"	NAMESPACE "acesup"